{
  "name": "jupyter-gemini-assistant",
  "displayName": "jupyter-gemini-assistant",
  "description": "",
  "version": "0.8.0",
  "publisher": "whoru",
  "repository": {
    "type": "git",
    "url": "https://github.com/IDKNWHORU/jupyter-gemini-assistant"
  },
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Notebooks",
    "AI"
  ],
  "icon": "icon.png",
  "activationEvents": [
    "onLanguage:jupyter",
    "onNotebook:jupyter-notebook"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jupyter.gemini.selectLanguage",
        "title": "Select Language for Jupyter Gemini assistant"
      },
      {
        "command": "jupyter.gemini.selectModel",
        "title": "Select Model for Jupyter Gemini assistant"
      }
    ],
    "configuration": {
      "title": "Jupyter Gemini Assistant",
      "properties": {
        "jupyterGeminiAssistant.language": {
          "type": "string",
          "default": "English",
          "enum": [
            "English",
            "한국어",
            "日本語",
            "Русский",
            "فارسی"
          ],
          "description": "The language used for Jupyter Gemini Assistant responses"
        },
        "jupyterGeminiAssistant.model": {
          "type": "string",
          "default": "gemini-flash-latest",
          "enum": [
            "gemini-3-flash-preview",
            "gemini-flash-latest",
            "gemini-flash-lite-latest"
          ],
          "description": "The model used for Jupyter Gemini Assistant"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.89.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "sinon": "^18.0.1",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "dotenv": "^17.2.1"
  }
}
